1) 
#include <iostream>
using namespace std;
int    sum(int a, int b) { return a + b; }
long   sum(long a, long b) { return a + b; }
float  sum(float a, float b) { return a + b; }
double sum(double a, double b) { return a + b; }
 
int main()
{
    int a = 2; int b = 3;
    cout << sum(a,b)<<endl;
 
    long a1 = 2; long b1 = 3;
    cout << sum(a1, b1) << endl;
 
    float a2 = 2; float b2 = 3;
    cout << sum(a2, b2) << endl;
 
    double a3 = 2; double b3 = 3;
    cout << sum(a3, b3) << endl;
 
 
    cin >> a;
    
}
2)
#include<iostream>

using namespace std;

int main()
{
 int n [5][10]; 
 srand(time(0)); 
 for (int i=0; i < 5; i++) 
 for (int j=0; j < 10; j++) 
 n [ i ][ j ] = rand(); 
 return 0; 
}
3)
onst unsigned int DIM1 = 3;
const unsigned int DIM2 = 5;
const unsigned int DIM3 = 2;

int ary[DIM1][DIM2][DIM3];

int main() {

    for (int i = 0; i < DIM1; i++) {
        for (int j = 0; j < DIM2; j++) {
            for (int k = 0; k < DIM3; k++) {
                ary[i][j][k] = (i + 1) * 100 + (j + 1) * 10 + (k + 1);
                cout << setw(4) << ary[i][j][k];
            }
            cout << endl;
        }
        cout << endl;
    }

    return 0;
}

