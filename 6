#include<iostream>

using namespace std;

int main()
{
 int n [5][10]; 
 srand(time(0)); 
 for (int i=0; i < 5; i++) 
 for (int j=0; j < 10; j++) 
 n [ i ][ j ] = rand(); 
 return 0; 
}
    
